___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');
const makeNumber = require('makeNumber');
const makeString = require('makeString');
const getType = require('getType');

function getAmountNumber(value) {
  return getType(value) === "number" ? value : makeNumber(makeString(value).replace(',', '.'));
}

if (data.id && data.amount) {
  
  const iAdvizeInterface = copyFromWindow('iAdvizeInterface') || [];
  
  iAdvizeInterface.push(function(iAdvize) {
    iAdvize.recordTransaction({
      id: data.id,
      amount: getAmountNumber(data.amount)
    });
  });
  
  setInWindow('iAdvizeInterface', iAdvizeInterface, true);
  
  data.gtmOnSuccess();
  
} else {
  
  data.gtmOnFailure();
  
}


